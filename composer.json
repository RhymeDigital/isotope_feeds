  "keywords":["contao", "isotope", "feed", "RSS 2.0", "Google Merchant"],
  "type":"contao-module",
  "license":"LGPL-3.0+",
  "authors":[
    {
      "name":"Rhyme Digital, LLC",
      "homepage":"http://rhyme.digital"
    }
  ],
  "require":{
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.9",
    "isotope/isotope-core": "^2.6"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.7"
  },
  "autoload": {
    "psr-4": {
        "Rhyme\\IsotopeFeedsBundle\\": "src"
    }
  },
  "extra":{
    "contao-manager-plugin": "Rhyme\\IsotopeFeedsBundle\\ContaoManager\\Plugin"
  }
